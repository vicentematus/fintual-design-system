{
  "name": "fintual-monorepo",
  "version": "1.0.0",
  "private": true,
  "description": "Fintual Design System Monorepo",
  "scripts": {
    "dev:web": "pnpm --filter @fintual/web dev",
    "dev:mobile": "pnpm --filter @fintual/mobile start",
    "dev:design-system": "pnpm --filter @fintual/design-system dev",
    "build:all": "pnpm -r build",
    "build:web": "pnpm --filter @fintual/web build",
    "build:mobile": "pnpm --filter @fintual/mobile build",
    "build:design-system": "pnpm --filter @fintual/design-system build",
    "clean": "pnpm -r exec rm -rf node_modules dist build",
    "test": "pnpm -r test",
    "lint": "pnpm -r lint",
    "ds:start": "pnpm --filter fintual-ds start",
    "ds:android": "pnpm --filter fintual-ds android",
    "ds:ios": "pnpm --filter fintual-ds ios",
    "ds:web": "pnpm --filter fintual-ds web",
    "ds:storybook": "pnpm --filter fintual-ds storybook",
    "ds:storybook:web": "pnpm --filter fintual-ds storybook:web",
    "ds:storybook:ios": "pnpm --filter fintual-ds storybook:ios",
    "ds:storybook:android": "pnpm --filter fintual-ds storybook:android",
    "ds:storybook:generate": "pnpm --filter fintual-ds storybook-generate",
    "ds:build-storybook": "pnpm --filter fintual-ds build-storybook"
  },
  "keywords": [
    "monorepo",
    "pnpm",
    "design-system",
    "fintual"
  ],
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=9.0.0"
  },
  "packageManager": "pnpm@9.0.0"
}