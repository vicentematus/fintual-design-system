{
  "name": "fintual-monorepo",
  "version": "1.0.0",
  "private": true,
  "description": "Fintual Design System Monorepo",
  "scripts": {
    "dev:web": "pnpm --filter @fintual/web dev",
    "dev:design-system": "pnpm --filter @fintual/design-system dev",
    "build:all": "pnpm -r build",
    "build:web": "pnpm --filter @fintual/web build",
    "build:design-system": "pnpm --filter @fintual/design-system build",
    "clean": "pnpm -r exec rm -rf node_modules dist build",
    "test": "pnpm -r test",
    "lint": "pnpm -r lint",
    "ds:start": "pnpm --filter fintual-ui start",
    "ds:android": "pnpm --filter fintual-ui android",
    "ds:ios": "pnpm --filter fintual-ui ios",
    "ds:web": "pnpm --filter fintual-ui web",
    "ds:storybook": "pnpm --filter fintual-ui storybook",
    "ds:storybook:web": "pnpm --filter fintual-ui storybook:web",
    "ds:storybook:ios": "pnpm --filter fintual-ui storybook:ios",
    "ds:storybook:android": "pnpm --filter fintual-ui storybook:android",
    "ds:storybook:generate": "pnpm --filter fintual-ui storybook-generate",
    "ds:build-storybook": "pnpm --filter fintual-ui build-storybook",
    "ds:bump": "pnpm --filter fintual-ui bump",
    "ds:prepublish": "pnpm --filter fintual-ui prepublish",
    "ds:publish": "pnpm --filter fintual-ui publish",
    "mobile:start": "pnpm --filter mobile start",
    "mobile:ios": "pnpm --filter mobile ios",
    "mobile:web": "pnpm --filter mobile web",
    "web:dev": "pnpm --filter web dev",
    "web:build": "pnpm --filter web build"
  },
  "keywords": [
    "monorepo",
    "pnpm",
    "design-system",
    "fintual"
  ],
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=10.0.0"
  },
  "packageManager": "pnpm@10.0.0",
  "pnpm": {
    "overrides": {
      "@types/react-dom": "~19.1.0",
      "react-docgen-typescript": "2.2.2"
    }
  }
}