{
  "name": "fintual-ui",
  "version": "1.0.0",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "react-native": "./src/index.ts",
  "license": "MIT",
  "sideEffects": false,
  "files": [
    "dist",
    "src",
    "README.md"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "react-native": "./src/index.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./tokens": {
      "types": "./dist/tokens/index.d.ts",
      "react-native": "./src/tokens/index.ts",
      "import": "./dist/tokens/index.mjs",
      "require": "./dist/tokens/index.js"
    },
    "./*": {
      "types": "./dist/components/*/index.d.ts",
      "react-native": "./src/components/*/index.ts",
      "import": "./dist/components/*/index.mjs",
      "require": "./dist/components/*/index.js"
    }
  },
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "storybook:web": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "storybook-generate": "sb-rn-get-stories",
    "storybook": "cross-env EXPO_PUBLIC_STORYBOOK_ENABLED='true' expo start",
    "storybook:ios": "cross-env EXPO_PUBLIC_STORYBOOK_ENABLED='true' expo start --ios",
    "storybook:android": "cross-env EXPO_PUBLIC_STORYBOOK_ENABLED='true' expo start --android",
    "clean": "rm -rf dist/",
    "build:types": "tsc --project tsconfig.build.json --emitDeclarationOnly --declaration --declarationMap",
    "build:js": "tsc --project tsconfig.build.json --outDir dist --module commonjs",
    "build:esm": "tsc --project tsconfig.build.json --outDir dist/temp-esm --module esnext && node scripts/rename-esm.js",
    "build": "pnpm clean && pnpm build:types && pnpm build:js && pnpm build:esm",
    "prepublishOnly": "pnpm build",
    "test:pack": "pnpm build && npm pack",
    "bump": "git add . && git commit -m 'chore: bump version' && npm version patch && pnpm publish",
    "publish": "npm publish --access public"
  },
  "dependencies": {
    "@gorhom/bottom-sheet": "^5.2.6",
    "@react-native-async-storage/async-storage": "2.2.0",
    "@react-native-community/datetimepicker": "8.4.4",
    "@react-native-community/slider": "5.0.1",
    "expo": "^54.0.20",
    "expo-constants": "~18.0.10",
    "expo-linking": "~8.0.8",
    "expo-router": "~6.0.13",
    "expo-splash-screen": "~31.0.10",
    "expo-status-bar": "~3.0.8",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "15.12.1",
    "react-native-web": "^0.21.0",
    "react-native-worklets": "0.5.1"
  },
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@babel/plugin-transform-class-static-block": "^7.27.1",
    "@babel/preset-env": "^7.25.4",
    "@chromatic-com/storybook": "^4.0.0",
    "@expo/metro-runtime": "~6.1.2",
    "@storybook/addon-docs": "^10.0.0",
    "@storybook/addon-ondevice-actions": "^10.0.0",
    "@storybook/addon-ondevice-backgrounds": "^10.0.0",
    "@storybook/addon-ondevice-controls": "^10.0.0",
    "@storybook/addon-ondevice-notes": "^10.0.0",
    "@storybook/react": "^10.0.0",
    "@storybook/react-native": "^10.0.0",
    "@storybook/react-native-web-vite": "^10.0.0",
    "@types/react": "~19.1.10",
    "babel-plugin-react-docgen-typescript": "^1.5.1",
    "cross-env": "^10.0.0",
    "storybook": "^10.0.0",
    "typescript": "~5.9.2",
    "vite": "^7.1.12"
  },
  "resolutions": {
    "react-docgen-typescript": "2.2.2"
  },
  "overrides": {
    "react-docgen-typescript": "2.2.2"
  },
  "pnpm": {
    "overrides": {
      "react-docgen-typescript": "2.2.2"
    }
  },
  "peerDependencies": {
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-native": ">=0.70.0",
    "react-native-web": ">=0.19.0"
  },
  "peerDependenciesMeta": {
    "react-native": {
      "optional": true
    },
    "react-native-web": {
      "optional": true
    }
  }
}